{
  "name": "showapp",
  "productName": "ShowApp",
  "version": "0.1.0",
  "description": "Desktop application for creating and viewing graduate showcase presentations",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "build": "rimraf release-builds && electron-packager . --asar=true --platform=win32 --arch=ia32 --out=release-builds --icon=src/assets/icons/win/icon-b.ico --prune=true --app-copyright=\"Copyright (c) 2017 Deltodon\" --win32metadata.CompanyName=Deltodon --win32metadata.FileDescription=ShowApp --win32metadata.ProductName=\"ShowApp Application\" --win32metadata.OriginalFilename=showapp.exe",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deltodon/ShowApp.git"
  },
  "keywords": [
    "showapp",
    "graduate",
    "showcase"
  ],
  "author": "Deltodon",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/deltodon/ShowApp/issues"
  },
  "homepage": "https://github.com/deltodon/ShowApp#readme",
  "devDependencies": {
    "electron": "^1.4.15",
    "electron-packager": "^8.5.2",
    "rimraf": "^2.6.1"
  }
}
